{
  "pluginName": "费率计算器",
  "description": "理财好帮手，费率计算器让每分钱都清晰。",
  "main": "index.html",
  "logo": "logo.png",
  "version": "1.0.0",
  "preload": "preload.js",
  "pluginSetting": {
    "single": true
  },
  "development": {
    "main": "http://localhost:5173"
  },
  "features": [
    {
      "code": "费率计算",
      "explain": "费率计算",
      "cmds": [
        "费率计算",
        {
          "type": "regex",
          "label": "费率计算",
          "match": "/^[1-9]\\d{0,9}(\\.\\d{2})?$/",
          "minLength": 1,
          "maxLength": 10
        }
      ]
    }
  ]
}